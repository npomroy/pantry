<div class="container">
  <div class="row">
    <div class="col-md-4 col-md-offset-4">
      <div class="well">
        <% if params[:plan] == '2' %>
          <h2>Pro Account Sign Up Form</h2>
          <%= render "pro_form" %>
        <% else %>
          <h2>Basic Account Sign Up Form</h2>
          <%= render "basic_form" %>
        <% end %>
        
        <%= render "devise/shared/links" %>
      </div>
    </div>
  </div>
</div>